    "firebase": "^4.13.1",
    "json-2-csv": "^2.2.0",
    "json2csv": "^4.1.2",
    "lodash": "^4.17.5",
    "moment": "^2.22.1",
    "react": "16.2.0",
    "react-bootstrap-tabs": "^1.0.2",
    "react-dom": "16.2.0",
    "react-paginate": "^5.2.2",
    "react-redux": "5.0.6",
    "react-router": "^3.2.1",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "3.7.2",
    "redux-observable": "0.17.0",
    "redux-thunk": "^2.2.0",
    "reselect": "3.0.1",
    "rxjs": "5.5.6",
    "tslib": "1.8.1",
    "typesafe-actions": "1.1.2",
    "utility-types": "1.0.0-rc.2"
  },
