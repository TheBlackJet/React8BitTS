    "css-loader": "^0.28.11",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.14.3",
    "jest": "22.1.1",
    "node-sass": "^4.8.3",
